#include <stdio.h>
int main(){
    float num1,num2,sum,prod,diff,quot;

    printf("Enter number1 : ");
    scanf("%f",&num1);
    printf("Enter number2 : ");
    scanf("%f",&num2);

    sum=num1+num2;
    prod=num1*num2;
    diff=num1-num2;
    quot=num1/num2;

    printf("\nThe sum of %.2f and %.2f is: %.2f",num1,num2,sum);
    printf("\nThe product of %.2f and %.2f is: %.2f",num1,num2,prod);
    printf("\nThe differnce of %.2f and %.2f is: %.2f",num1,num2,diff);
    printf("\nThe quotient of %.2f and %.2f is: %.2f",num1,num2,quot);

    return 0;
}
